{
  "name": "cars-portfolio",
  "version": "1.0.0",
  "description": "Cars Portfolio Application",
  "main": "./build/app.js",
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "jest",
    "local": "ts-node-dev --respawn --transpileOnly src/cars-portfolio-server/app.ts",
    "build": "tsc && cd src/cars-portfolio-client && npm install --only=dev && npm install && npm run build",
    "start": "node ./build/app.js"
  },
  "keywords": [
    "node",
    "express",
    "typescript",
    "rxjs",
    "dynamodb"
  ],
  "author": "Rolan Burykin",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.2",
    "aws-sdk": "^2.593.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "rxjs": "^6.5.3",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/jest": "^24.0.21",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-mockito": "^2.5.0",
    "ts-node-dev": "^1.0.0-pre.43"
  }
}
