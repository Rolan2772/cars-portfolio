{
  "name": "ts-cars-portfolio-docker",
  "version": "1.0.0",
  "description": "Docker configuration for ts-cars-portfolio project",
  "scripts": {
    "tsc": "tsc && copyfiles -u 1 \"src/data/*.json\" build",
    "import": "tsc && node build/import.js",
    "create-table": "tsc && node build/createTable.js",
    "delete-table": "tsc && node build/deleteTable.js",
    "create-local-db": "npm run tsc && npm run create-table && npm run import",
    "create-remote-db": "npm run tsc && npm run create-table && npm run import"
  },
  "author": "Rolan Burykin",
  "license": "ISC",
  "keywords": [
    "docker",
    "docker-compose",
    "nodejs",
    "dynamodb",
    "aws-sdk"
  ],
  "devDependencies": {
    "@types/node": "^13.1.1",
    "aws-sdk": "^2.596.0",
    "copyfiles": "^2.1.1",
    "typescript": "^3.7.4"
  }
}
