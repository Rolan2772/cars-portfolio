{
  "include": [
    "src/**.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false,
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "build"
  },
  "lib": ["es2015"]
}